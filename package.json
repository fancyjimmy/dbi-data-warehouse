		"@sveltejs/kit": "^1.20.4",
		"@tailwindcss/typography": "^0.5.10",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"flowbite": "^2.1.1",
		"flowbite-svelte": "^0.44.19",
		"flowbite-svelte-icons": "^0.4.5",
		"postcss": "^8.4.24",
